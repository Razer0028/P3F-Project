[onprem]
onprem-1 ansible_host=<onprem_ip> ansible_user=root ansible_ssh_private_key_file=~/.ssh/onprem_ed25519

[vps]
vps-1 ansible_host=<vps_ip> ansible_user=root ansible_ssh_private_key_file=~/.ssh/vps_ed25519

[ec2]
ec2-1 ansible_host=<ec2_ip> ansible_user=admin ansible_ssh_private_key_file=~/.ssh/ec2_key.pem

[all:vars]
ansible_python_interpreter=/usr/bin/python3
